{
  "presets": [
    "env",
    "react"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "src": "./src",
          "containers": "src/containers",
          "components": "src/components"
        }
      }
    ]
  ]
}